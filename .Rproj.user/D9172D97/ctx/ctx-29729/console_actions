        "+ ",
        "  file <- paste0(\"season-\", season, \".csv\")",
        "+ ",
        "  ",
        "+ ",
        "  if (file.exists(file)) {",
        "+ ",
        "    df <- read_csv(file, show_col_types = FALSE)",
        "+ ",
        "    ",
        "+ ",
        "    if (all(c(\"FTHG\", \"FTAG\") %in% names(df))) {",
        "+ ",
        "      epl_season_data[[season]] <- df[, c(\"FTHG\", \"FTAG\")]",
        "+ ",
        "      names(epl_season_data[[season]]) <- c(\"X\", \"Y\")",
        "+ ",
        "    } else {",
        "+ ",
