



true_params <- list(n1 = 2, n2 = 3, p1 = 0.6, p2 = 0.4, lambda = 0.2)
data <- rbinomBCD(n = 1000,n1 = true_params$n1, n2 = true_params$n2, p1 = true_params$p1, p2 = true_params$p2, lambda = true_params$lambda)
MLEbinomBCD(data)
